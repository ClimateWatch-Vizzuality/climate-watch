    "react-player": "0.20.0",
    "react-redux": "^5.0.5",
    "react-router": "4.2.0",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.0",
    "react-router-hash-link": "github:j8seangel/react-router-hash-link",
    "react-selectize": "3.0.1",
    "react-simple-maps": "0.8.3",
    "react-stickynode": "^1.3.1",
    "react-tooltip": "3.3.0",
