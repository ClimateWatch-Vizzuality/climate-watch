{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "properties": {
    "data": {
      "$id": "/properties/data",
      "type": "array",
      "items": {
        "$id": "/properties/data/items",
        "type": "object",
        "properties": {
          "id": {
            "$id": "/properties/data/items/properties/id",
            "type": "integer",
            "title": "The Id Schema ",
            "default": 0,
            "examples": [
              66310
            ]
          },
          "region": {
            "$id": "/properties/data/items/properties/region",
            "type": "string",
            "title": "The Region Schema ",
            "default": "",
            "examples": [
              "Aruba"
            ]
          },
          "iso_code3": {
            "$id": "/properties/data/items/properties/iso_code3",
            "type": "string",
            "title": "The Iso_code3 Schema ",
            "default": "",
            "examples": [
              "ABW"
            ]
          },
          "data_source": {
            "$id": "/properties/data/items/properties/data_source",
            "type": "string",
            "title": "The Data_source Schema ",
            "default": "",
            "examples": [
              "PIK"
            ]
          },
          "gwp": {
            "$id": "/properties/data/items/properties/gwp",
            "type": "string",
            "title": "The Gwp Schema ",
            "default": "",
            "examples": [
              "AR2"
            ]
          },
          "sector": {
            "$id": "/properties/data/items/properties/sector",
            "type": "string",
            "title": "The Sector Schema ",
            "default": "",
            "examples": [
              "Total including LULUCF"
            ]
          },
          "gas": {
            "$id": "/properties/data/items/properties/gas",
            "type": "string",
            "title": "The Gas Schema ",
            "default": "",
            "examples": [
              "CH4"
            ]
          },
          "unit": {
            "$id": "/properties/data/items/properties/unit",
            "type": "string",
            "title": "The Unit Schema ",
            "default": "",
            "examples": [
              "C02e"
            ]
          },
          "emissions": {
            "$id": "/properties/data/items/properties/emissions",
            "type": "array",
            "items": {
              "$id": "/properties/data/items/properties/emissions/items",
              "type": "object",
              "properties": {
                "year": {
                  "$id": "/properties/data/items/properties/emissions/items/properties/year",
                  "type": "integer",
                  "title": "The Year Schema ",
                  "default": 0,
                  "examples": [
                    2011
                  ]
                },
                "value": {
                  "$id": "/properties/data/items/properties/emissions/items/properties/value",
                  "type": "number",
                  "title": "The Value Schema ",
                  "default": 0,
                  "examples": [
                    0.02246999926865101
                  ]
                }
              }
            }
          }
        }
      }
    },
    "meta": {
      "$id": "/properties/meta",
      "type": "object",
      "properties": {
        "years": {
          "$id": "/properties/meta/properties/years",
          "type": "array",
          "items": {
            "$id": "/properties/meta/properties/years/items",
            "type": "integer",
            "title": "The 0th Schema ",
            "default": 0,
            "examples": [
              1990,
              1991,
              1992,
              1993,
              1994,
              1995,
              1996,
              1997,
              1998,
              1999,
              2000,
              2001,
              2002,
              2003,
              2004,
              2005,
              2006,
              2007,
              2008,
              2009,
              2010,
              2011,
              2012,
              2013,
              2014,
              2015
            ]
          }
        },
        "columns": {
          "$id": "/properties/meta/properties/columns",
          "type": "array",
          "items": {
            "$id": "/properties/meta/properties/columns/items",
            "type": "object",
            "properties": {
              "name": {
                "$id": "/properties/meta/properties/columns/items/properties/name",
                "type": "string",
                "title": "The Name Schema ",
                "default": "",
                "examples": [
                  "gas"
                ]
              },
              "sortable": {
                "$id": "/properties/meta/properties/columns/items/properties/sortable",
                "type": "boolean",
                "title": "The Sortable Schema ",
                "default": false,
                "examples": [
                  true
                ]
              },
              "current": {
                "$id": "/properties/meta/properties/columns/items/properties/current",
                "type": ["string", "null"],
                "title": "The Current Schema ",
                "default": "",
                "examples": [
                  "ASC"
                ]
              }
            }
          }
        }
      }
    }
  }
}
