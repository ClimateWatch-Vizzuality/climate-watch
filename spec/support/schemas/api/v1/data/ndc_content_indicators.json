{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "properties": {
    "data": {
      "$id": "/properties/data",
      "type": "array",
      "items": {
        "$id": "/properties/data/items",
        "type": "object",
        "properties": {
          "id": {
            "$id": "/properties/data/items/properties/id",
            "type": "integer",
            "title": "The Id Schema ",
            "default": 0,
            "examples": [
              6709
            ]
          },
          "slug": {
            "$id": "/properties/data/items/properties/slug",
            "type": "string",
            "title": "The Slug Schema ",
            "default": "",
            "examples": [
              "indc_summary"
            ]
          },
          "name": {
            "$id": "/properties/data/items/properties/name",
            "type": "string",
            "title": "The Name Schema ",
            "default": "",
            "examples": [
              "(I)NDC summary"
            ]
          },
          "description": {
            "$id": "/properties/data/items/properties/description",
            "type": "null",
            "title": "The Description Schema ",
            "default": null,
            "examples": [
              null
            ]
          },
          "source_id": {
            "$id": "/properties/data/items/properties/source_id",
            "type": "integer",
            "title": "The Source_id Schema ",
            "default": 0,
            "examples": [
              53
            ]
          },
          "category_ids": {
            "$id": "/properties/data/items/properties/category_ids",
            "type": "array",
            "items": {
              "$id": "/properties/data/items/properties/category_ids/items",
              "type": "integer",
              "title": "The 0th Schema ",
              "default": 0,
              "examples": [
                821,
                817
              ]
            }
          }
        }
      }
    }
  }
}
