{
  "$id": "http://example.com/example.json",
  "type": "array",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "items": {
    "$id": "http://example.com/example.json/items",
    "type": "object",
    "properties": {
      "id": {
        "$id": "http://example.com/example.json/items/properties/id",
        "type": "integer",
        "title": "The Id Schema ",
        "default": 0,
        "examples": [
          281248
        ]
      },
      "country": {
        "$id": "http://example.com/example.json/items/properties/country",
        "type": "string",
        "title": "The Country Schema ",
        "default": "",
        "examples": [
          "Afghanistan"
        ]
      },
      "iso_code3": {
        "$id": "http://example.com/example.json/items/properties/iso_code3",
        "type": "string",
        "title": "The Iso_code3 Schema ",
        "default": "",
        "examples": [
          "AFG"
        ]
      },
      "indc_text": {
        "$id": "http://example.com/example.json/items/properties/indc_text",
        "type": "string",
        "title": "The Indc_text Schema ",
        "default": "",
        "examples": [
          "South-south collaboration on low-carbon agriculture, study tours."
        ]
      },
      "status": {
        "$id": "http://example.com/example.json/items/properties/status",
        "type": "string",
        "title": "The Status Schema ",
        "default": "",
        "examples": [
          "Future"
        ]
      },
      "climate_response": {
        "$id": "http://example.com/example.json/items/properties/climate_response",
        "type": "string",
        "title": "The Climate_response Schema ",
        "default": "",
        "examples": [
          "Adaptation"
        ]
      },
      "type_of_information": {
        "$id": "http://example.com/example.json/items/properties/type_of_information",
        "type": "string",
        "title": "The Type_of_information Schema ",
        "default": "",
        "examples": [
          "Needs & Gaps"
        ]
      },
      "sector": {
        "$id": "http://example.com/example.json/items/properties/sector",
        "type": "string",
        "title": "The Sector Schema ",
        "default": "",
        "examples": [
          "Forest and land use"
        ]
      },
      "target_number": {
        "$id": "http://example.com/example.json/items/properties/target_number",
        "type": "string",
        "title": "The Target_number Schema ",
        "default": "",
        "examples": [
          "17.6"
        ]
      },
      "target": {
        "$id": "http://example.com/example.json/items/properties/target",
        "type": "string",
        "title": "The Target Schema ",
        "default": "",
        "examples": [
          "Enhance North-South, South-South and triangular regional and international cooperation on and access to science, technology and innovation and enhance knowledge sharing on mutually agreed terms, including through improved coordination among existing mechanisms, in particular at the United Nations level, and through a global technology facilitation mechanism"
        ]
      },
      "goal_number": {
        "$id": "http://example.com/example.json/items/properties/goal_number",
        "type": "string",
        "title": "The Goal_number Schema ",
        "default": "",
        "examples": [
          "17"
        ]
      },
      "goal": {
        "$id": "http://example.com/example.json/items/properties/goal",
        "type": "string",
        "title": "The Goal Schema ",
        "default": "",
        "examples": [
          "Strengthen the means of implementation and revitalize the global partnership for sustainable development"
        ]
      }
    }
  }
}
